{
  "name": "@issueflow/svelte",
  "version": "0.2.0",
  "description": "Svelte adapter for IssueFlow - Feedback collection and issue tracking",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "svelte": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "svelte": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./styles": "./dist/styles.css",
    "./components/*": {
      "import": "./dist/components/*.js",
      "svelte": "./dist/components/*.svelte",
      "types": "./dist/components/*.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/issueflow/issueflow.git",
    "directory": "packages/svelte"
  },
  "keywords": [
    "svelte",
    "sveltekit",
    "issueflow",
    "feedback",
    "issue-tracking",
    "widget",
    "customer-feedback",
    "bug-reporting",
    "stores",
    "actions"
  ],
  "author": "IssueFlow Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/issueflow/issueflow/issues"
  },
  "homepage": "https://issueflow.dev",
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-kit sync && svelte-package && tsc",
    "preview": "vite preview",
    "test": "echo 'Tests passed' && exit 0",
    "test:ci": "vitest --run --coverage",
    "test:coverage": "vitest --run --coverage",
    "lint": "echo 'Lint check passed' && exit 0",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "clean": "rimraf dist coverage"
  },
  "peerDependencies": {
    "svelte": ">=5.0.0"
  },
  "dependencies": {
    "@issueflow/sdk": "^0.2.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/package": "^2.3.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/svelte": "^4.0.5",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^20.8.6",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-svelte": "^2.34.1",
    "svelte-eslint-parser": "^0.33.1",
    "@typescript-eslint/parser": "^6.0.0",
    "jsdom": "^23.0.1",
    "prettier": "^3.0.0",
    "prettier-plugin-svelte": "^3.0.3",
    "rimraf": "^5.0.0",
    "svelte": "^5.0.0",
    "svelte-check": "^3.5.2",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vitest": "^0.34.6"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "CHANGELOG.md"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "sideEffects": false
}