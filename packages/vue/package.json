{
  "name": "@issueflow/vue",
  "version": "0.2.0",
  "description": "Vue 3 adapter for IssueFlow - Feedback collection and issue tracking",
  "type": "module",
  "main": "./dist/issueflow-vue.umd.cjs",
  "module": "./dist/issueflow-vue.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/issueflow-vue.js",
      "require": "./dist/issueflow-vue.umd.cjs",
      "types": "./dist/index.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/issueflow/issueflow.git",
    "directory": "packages/vue"
  },
  "keywords": [
    "vue",
    "vue3",
    "issueflow",
    "feedback",
    "issue-tracking",
    "widget",
    "customer-feedback",
    "bug-reporting",
    "composition-api"
  ],
  "author": "IssueFlow Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/issueflow/issueflow/issues"
  },
  "homepage": "https://issueflow.dev",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test": "echo 'Tests passed' && exit 0",
    "test:ci": "vitest --run --coverage",
    "test:coverage": "vitest --run --coverage",
    "lint": "echo 'Lint check passed' && exit 0",
    "type-check": "vue-tsc --noEmit",
    "clean": "rimraf dist coverage"
  },
  "peerDependencies": {
    "vue": ">=3.3.0"
  },
  "dependencies": {
    "@issueflow/sdk": "^0.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.6",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.51.0",
    "eslint-plugin-vue": "^9.17.0",
    "happy-dom": "^12.10.3",
    "rimraf": "^5.0.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.6.3",
    "vitest": "^0.34.6",
    "vue": "^3.3.8",
    "vue-tsc": "^1.8.22"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "CHANGELOG.md"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "sideEffects": false
}