{
  "name": "@issueflow/angular",
  "version": "0.2.0",
  "description": "Angular adapter for IssueFlow - Feedback collection and issue tracking",
  "main": "dist/bundles/issueflow-angular.umd.js",
  "module": "dist/fesm2022/issueflow-angular.mjs",
  "typings": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "esm2022": "./dist/esm2022/issueflow-angular.mjs",
      "esm": "./dist/esm2022/issueflow-angular.mjs",
      "default": "./dist/fesm2022/issueflow-angular.mjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/issueflow/issueflow.git",
    "directory": "packages/angular"
  },
  "keywords": [
    "angular",
    "issueflow",
    "feedback",
    "issue-tracking",
    "widget",
    "customer-feedback",
    "bug-reporting"
  ],
  "author": "IssueFlow Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/issueflow/issueflow/issues"
  },
  "homepage": "https://issueflow.dev",
  "scripts": {
    "build": "ng-packagr -p ng-package.json",
    "build:watch": "ng-packagr -p ng-package.json --watch",
    "test": "ng test",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadless",
    "lint": "ng lint",
    "clean": "rimraf dist"
  },
  "peerDependencies": {
    "@angular/animations": ">=15.0.0",
    "@angular/common": ">=15.0.0",
    "@angular/core": ">=15.0.0",
    "@angular/forms": ">=15.0.0",
    "rxjs": ">=7.0.0",
    "tslib": ">=2.0.0"
  },
  "dependencies": {
    "@issueflow/sdk": "workspace:*"
  },
  "devDependencies": {
    "@angular/animations": "^17.0.0",
    "@angular/common": "^17.0.0",
    "@angular/compiler": "^17.0.0",
    "@angular/core": "^17.0.0",
    "@angular/forms": "^17.0.0",
    "@angular/platform-browser": "^17.0.0",
    "@angular/platform-browser-dynamic": "^17.0.0",
    "@types/jasmine": "^5.1.0",
    "@types/node": "^20.8.6",
    "jasmine": "^5.1.0",
    "karma": "^6.4.0",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.0",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "ng-packagr": "^17.0.0",
    "rimraf": "^5.0.0",
    "rxjs": "^7.8.0",
    "tslib": "^2.6.0",
    "typescript": "^5.2.2"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "CHANGELOG.md"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "sideEffects": false
}